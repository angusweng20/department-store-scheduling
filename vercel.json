{
  "buildCommand": "cd frontend-vite && npm run build",
  "outputDirectory": "frontend-vite/dist",
  "installCommand": "cd frontend-vite && npm install",
  "rewrites": [
    {
      "source": "/system-admin",
      "destination": "/system-admin/index.html"
    },
    {
      "source": "/system-admin/:path*",
      "destination": "/system-admin/:path*"
    }
  ],
  "headers": [
    {
      "source": "/system-admin/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
