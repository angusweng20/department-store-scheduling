{
  "buildCommand": "cd frontend-vite && npm run build",
  "outputDirectory": "frontend-vite/dist",
  "installCommand": "cd frontend-vite && npm install",
  "env": {
    "VITE_SUPABASE_URL": "@vite_supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@vite_supabase_anon_key"
  },
  "rewrites": [
    {
      "source": "/system-admin",
      "destination": "/index.html"
    },
    {
      "source": "/system-admin/(.*)",
      "destination": "/(.*)"
    }
  ]
}
